    "axios": "^1.5.1",
    "expo": "~49.0.12",
    "expo-av": "~13.4.1",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.17",
    "formik": "^2.4.5",
